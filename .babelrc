{
    // "presets": ["es2015",
    //     "react",
    //     "stage-2",
    //     "env"],
    "presets": ["es2017","env","react","stage-2"],
    "plugins": [
      ["import", { "libraryName": "antd", "style": "css" }], // `style: true` for lessï¼Œ
      "transform-decorators-legacy"
    ],
    "env": {
      "development": {
      "plugins": [["react-transform", {
         "transforms": [{
           "transform": "react-transform-hmr",
           
           "imports": ["react"],
           
           "locals": ["module"]
         }]
       }]]
      }
    }
  }