{
    "presets": ["es2017",
        "react",
        "stage-3",
        "env"],
    "plugins": [
      ["import", { "libraryName": "antd", "style": "css" }], // `style: true` for less，
      "transform-decorators-legacy"
    ],
    "env": {
      "production": {
          "presets": ["react-optimize"]
      },
      "development": {
          "presets": ["react-hmre"]
      }
  }
